#
# Execute-Link format file: ELF
#

menu "ELF: Executable and Linkable Format"

config DEBUG_BINARY_ELF
	bool "ELF: Executable and Linkable Format"
	help
	  Executable and Linkable Format (ELF, formerly named Extensible 
	  Linking Format), is a common standard file format for executable
	  files, object code, shared libraries, and core dumps. 

if DEBUG_BINARY_ELF

config ELF_RELOCATABLE_FILE
	bool "Parse ELF Relocable file"
	help
	  Parse formt for Relocable file.

config DEBUG_ELF_FORMAT
	bool "Format for ELF (Executable and Linkable Format)"
	help
	  Parse foramt for ELF (Executable and Linkable Format).

config PARSE_ELF_HEADER
	bool "Parse Header for ELF"
	help
	  Parse ELF header.

endif

endmenu
