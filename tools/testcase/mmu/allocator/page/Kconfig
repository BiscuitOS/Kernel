#
# Get one free page
#

menu "Free page memory allocator"

config DEBUG_PAGE_ALLOCATOR
	bool "Free Page Memory Allocator"
	help
	  Debug free page memory allocator.

if DEBUG_PAGE_ALLOCATOR

config GET_FREE_ZERO_PAGE
	bool "Obtain one free zero page"
	help
	  Obtain one free zero page.

endif

endmenu
